{
  "cli": {
    "version": ">= 7.3.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "channel": "preview",  // Ajoutez cette ligne sous le profil concerné
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "channel": "production",  // Assurez-vous que ce canal est configuré
      "android": {
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
